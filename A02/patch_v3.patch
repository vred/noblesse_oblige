+  */
+ asmlinkage int sys_incshare()
+ {
+ 	struct task_struct *p;
+ 	//Find the current task
+ 	for_each_task(p) {
+ 		if (!proc_sel(p, 0, 0))
+ 			continue;
+ 		atomic_inc(&p->share);
+ 		atomic_inc(&p->user->processes);
+ 	}
+ 	return 0;
+ }
